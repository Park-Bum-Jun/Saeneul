<li class="alarm ui-li-static li-has-multiline li-has-right-circle-btn"
    data-alarmid="%id%">
    %name%
    <span class="li-text-sub">%startTime% - %daysText%</span>
    <div class="removeAlarm ui-btn" data-inline="true" data-icon="remove"
         data-style="circle" data-alarmid="%id%"></div>
</li>